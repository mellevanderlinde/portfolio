{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "outputs": [".next/**", "out/**", "src/index.js"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "lint": {},
    "test": {},
    "test:update": {},
    "infra#test": {
      "dependsOn": ["infra#build"]
    },
    "infra#test:update": {
      "dependsOn": ["infra#build"]
    }
  }
}
