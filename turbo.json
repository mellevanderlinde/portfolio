{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["out/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "lint:fix": {
      "dependsOn": ["^lint:fix"]
    },
    "test": {
      "dependsOn": ["^test", "website#build"]
    },
    "test:update": {
      "dependsOn": ["^test:update", "website#build"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
